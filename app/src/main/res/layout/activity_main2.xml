        android:layout_marginEnd="90dp"
        android:layout_marginRight="90dp"
        android:text="진동주기2번,       세탁이 끝났어요!"
        android:textSize="17sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
